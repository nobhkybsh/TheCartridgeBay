	pause_frames(30)
	for i = 1,5 do	
		for s in all(stacks_supply) do
			-- ensure that the last click on the deck of cards places a card on each middle stack
			if i ~= 5 or #deck_stack.cards % #stacks_supply ~= 0 then
				local c = get_top_card(deck_stack)
				stack_add_card(s, c)
				c.a_to = 0.5
				pause_frames(3)
			end
		end
		pause_frames(5)
