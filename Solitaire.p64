function ui_boxes(x, y, n)
	ui_top_edge(x+2, y)
	ui_top_edge(x+60, y)
	rectfill(x+14, y+3, x+60, y+3, 6) 

	for i = 0,n do
		local yi = y+i*70
		ui_left_edge(x, yi+3)
		ui_right_edge(x+70, yi+3)
		spr(273, x+4, yi+4)
		spr(273, x+62, yi+4)
	
		if i > 0 then
			rectfill(x+3, yi-58, x+3, yi+2, 6)
			rectfill(x+69, yi-58, x+69, yi+2, 5)
		end
	end
	
	local yn = y+n*70
