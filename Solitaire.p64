		wrap_stack_resolve(s)
	end
	
	stack_off_frame = stack_new(
		{},
		-card_width*2,
		-card_height*2,
		{
			reposition = stack_repose_normal(0)
		})

	init_menus()
	
	button_new({ 
		x = 30, y = 163,
		width = 41, height = 13,
		on_click = function(b)
			if not game_over then
