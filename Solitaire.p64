function ui_numbers(x, y, n)
	assert(n >= 0 and n%1 == 0, "invalid number")
	
	repeat
		local v = n%10
		n \= 10
		
		sspr(266, v*5,0, 6,10, x,y)
		
		x -= 7
	until n == 0
end

